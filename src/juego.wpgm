import wollok.game.*
import AutoJugador.*
import Calle.*

program juego {
	game.title("GRA: Gran Robo Automotor")
	game.cellSize(100)
	game.boardGround("Calle1.png")
	game.height(9)
	game.width(6)
	var autoJugador = new AutoJugador(vida = 100, image = "Auto_Derecho.png", position = game.at(0, 0))
	game.addVisualCharacter(autoJugador)
	game.start()
}
